					-o-transition: all 0.3s ease-in-out 0s;
					transition: all 0.3s ease-in-out 0s;
					margin:-1px 0 0 !important;
					border:1px solid #666;
					padding:0.4em 0em 0.4em 1em;
					background: #555 url("../images/plus.png") no-repeat center right;
				}
				.footer-link-title:hover{
					background: #777 url("../images/plus.png") no-repeat center right;
				}
				.footer-link-title.active{
					background: #666 url("../images/delete.png") no-repeat center right;
				}
				.footer-children-wrapper{
					display:none;
					float:right !important;
					width:100%;
					background: white !important;
					.footer-link-item{
						display: inline-block;
						width: 100%;
						margin:0.1em;
					}
				}
			}
		}
		.node-footer{
		  padding-top:10px !important;
		}
	}
	
	////////////
	
	.breadcrumbs{
		margin-bottom: 0px !important;
	}
	
	nav{
		height:50px !important;
	}

	.top-bar.expanded{
		height:auto !important;
	}
	
	.top-bar{
		.title-area{
			.name{
				.site-title{
				.site-name{
					display:block;
				}
				.site-logo{
					display:none;
				}
					line-height: 40px;
					margin-left: 15px;
					font-size: 1.0625em !important;
					font-weight:bold;
					.pone{
						display:none;
					}
					a{
						color:white !important;
					}
				}
			}
		}
	}

	.node-front-page-text{
		width:100%;
		text-align:left !important;
	}

	.middle-head-menu{
		//display:none;
	}

	.top-bar{
		.top-bar-section{
			.right.menu-base-menu{
				display:none;
			}
			.main-menu-links{
				display:block !important;
				.right.main-menu{
					display: block;
				}
			}
		}
	}

	.webform-component {
		padding:10px;
		padding-bottom:2px;
		width:100%!important;
	}

	.form-item-pass-pass1{
		width:100% !important;
	}
	.form-item-pass-pass2{
		width:100% !important;
	}
	.form-item-pass{
		width:100% !important;
	}
	.password-suggestions{
		width:100% !important;
	}

	.flex-video {
		position:relative;
		padding-top:25px;
		padding-bottom:67.5%;
		height:350px!important;
		margin-bottom:16px;
		overflow:hidden;
		z-index:50;
	}
	
	
	.flex-video {
		width:50%;
		padding-bottom:10px!important;
		margin-bottom:none!important;
	}
	
	.block.block-nodeblock.block-nodeblock-10.header {
		width:100%;
	}
	
	.block.block-nodeblock.contextual-links-region.block-nodeblock-10.header {
		float:left;
		width:100%;
	}
	
	.menu {
		width:100%!important;
		padding-right:0;
	}
	
	.menu li {
		float:left;
	}
	
	.block.block-nodeblock.block-nodeblock-10.header {
		float:left;
		width:100%;
	}
	
	.block.block-nodeblock.contextual-links-region.block-nodeblock-10.header {
		float:left;
		width:100%;
	}
	
	#search-block-form {
		padding-top:5px;
		padding-right:30px!important;
	}

	
	.main-nav.active-fixed {
		position:fixed!important;
		margin-left:0!important;
		float:left;
	}
	
	
	.main-nav {
		width:80%;
		position:relative!important;
	}
	
	.block.block-nodeblock.block-nodeblock-10.header {
		width:100%;
	}
	
	.block.block-nodeblock.contextual-links-region.block-nodeblock-10.header {
		width:100%;
	}
