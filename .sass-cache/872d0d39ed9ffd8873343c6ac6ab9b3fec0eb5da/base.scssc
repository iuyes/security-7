/*Save FIles*/
.portlet-content{
	float:left;
	width:100%;
}
.save-file{
	cursor: pointer;
	color:#36a7c9 !important;
}
.save-file:hover{
	cursor: pointer;
	color:#04c5ff !important;
}
.dropped{
	background: #d0d0d0;
}
.save{
	padding-top:5px;
	text-align: center;
}
.sort-saved-files{
	float:right;
}
.ui-save-active{
	
}

.save-files{
	box-shadow:0 5px 5px 5px #888;
	float:right;
	height:100px;
	display:none;
	position:fixed;
	top:200px;
	right:100px;
	width:130px !important;
	background: url(/sites/all/themes/fortified_dev/images/folder.png) no-repeat center transparent;
}






/* File Browser */
.file-browser-wrapper{
  padding-top:50px;
  .item-list{
