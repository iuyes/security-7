    }
    .view-file{
      cursor:pointer;
    }
    .view-file:hover{
        background:#f3f3f3;
      }
    .file-list{
      float:left;
      list-style-type: none;
    }
  }
}


#file-preview{
  padding-top:20px;
  list-style-type: none;
  li{
    cursor:pointer;
    max-width:200px;
    float:left;
  }
}

#file-preview{
  padding-top:20px;
}

.file-actions-wrapper{
  list-style-type: none;
  .file-actions{
    float:left;
    display:none;
  }
}
