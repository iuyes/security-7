/*GLOBAL*/


@mixin menu-style($background){
   		 border-style: solid;
			border-width: 1px;
			cursor: pointer;
			font-family: inherit;
			font-weight: bold;
			line-height: 1;
			margin: 0 0 1.25em;
			position: relative;
			text-decoration: none;
			text-align: center;
			display: inline-block;
			font-size: 1em;
			background-color: #2ba6cb;
			border-color: #2284a1;
			color: white;
			-webkit-box-shadow: 0 1px 0 rgba(255, 255, 255, 0.5) inset;
			box-shadow: 0 1px 0 rgba(255, 255, 255, 0.5) inset;
			-webkit-transition: all 300ms ease-out;
			-moz-transition: all 300ms ease-out;
			transition: all 300ms ease-out;
			font-size: 0.8125em;
			margin-bottom: 0;
			background-color: $background;
			border-color: #d0d0d0;
			color: #333333;
			padding: .50em 1.00em !important;
}


@mixin h3-styles($colors){
	font-weight:bold;
	font-size:22px;
	color:$colors;
	font-style: italic !important;
	font-family:$font !important;
	padding-bottom:5px;
}


// custom f-tooltip

@mixin f-tooltip-arrow-up($top, $left){
	content: "";
	display: block;
	width: 0;
	height: 0;
	border: inset 6px;
	border-color:transparent transparent #fff transparent;
	border-bottom-style: solid;
	position: absolute;
	top: $top; /* -12px; */
	left: $left; /* 10px; */
	z-index: 99999;
}

@mixin f-tooltip-arrow-down(){
	content: "";
	width: 0; 
	height: 0;
	border-left: 20px solid transparent;
	border-right: 20px solid transparent;
	border-top: 20px solid #fff;
	position: absolute;
	bottom: -5px; /* -12px; */
	left:50%;
	z-index: 99999;
}

.tooltip-throbber{
	//min-height:35px;
	padding-left:35px;
	background: #fff url(../images/throbber.gif) no-repeat 70% 70%;
	display:inline;
	//background-attachment:fixed;
}

.close-tooltip {
	float:right;
}
.close-tooltip:hover{
	background: #d0d0d0 !important;
	cursor:pointer;
}


.f-tooltip-wrapper{
	position: relative;
	z-index: 99999 !important;
}
.f-tooltip-open{
	box-shadow:0 5px 20px 5px #666;
	background:white;
	position: absolute;
	border-radius:5px;
	z-index: 99999 !important;
	padding:15px;
}

.f-tooltip-medium{
	min-width:450px !important;
	float:left;
}

.f-tooltip-small{
	max-width:300px;
	float:left;
}

.f-tooltip-large{
	min-width:650px;
	max-width:700px;
	float:left;
}
.f-tooltip-auto{
	min-width:600px;
	float:left;
}

.tooltip-before:before{
	@include f-tooltip-arrow-up(-12px, 10px);
}

.tooltip-after:after{
	@include f-tooltip-arrow-down();
}
.ui-accordion-content.ui-helper-reset.ui-widget-content.ui-corner-bottom.ui-accordion-content-active{
	/* overflow: visible !important; */
	overflow: scroll !important;
}

// end custom f-tooltip


