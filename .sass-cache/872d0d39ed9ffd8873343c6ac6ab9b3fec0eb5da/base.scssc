	.top-bar{
		.top-bar-section{
			.right.menu-base-menu{
				display:none;
			}
			.main-menu-links{
				display:block !important;
				.right.main-menu{
					display: block;
				}
			}
		}
	}

	.webform-component {
	padding:10px;
	padding-bottom:2px;
	width:100%!important;
}

	.form-item-pass-pass1{
	width:100% !important;
}
.form-item-pass-pass2{
	width:100% !important;
}
.form-item-pass{
	width:100% !important;
}
.password-suggestions{
	width:100% !important;
}

	.flex-video {
		position:relative;
		padding-top:25px;
		padding-bottom:67.5%;
		height:350px!important;
		margin-bottom:16px;
		overflow:hidden;
		z-index:50;
	}
	
	
	.flex-video {
		width:50%;
		padding-bottom:10px!important;
		margin-bottom:none!important;
	}
	
	.block.block-nodeblock.block-nodeblock-10.header {
		width:100%;
	}
	
	.block.block-nodeblock.contextual-links-region.block-nodeblock-10.header {
		float:left;
		width:100%;
	}
	
	.menu {
		width:100%!important;
		padding-right:0;
	}
	
	.menu li {
		float:left;
	}
	
	.block.block-nodeblock.block-nodeblock-10.header {
		float:left;
		width:100%;
	}
	
	.block.block-nodeblock.contextual-links-region.block-nodeblock-10.header {
		float:left;
		width:100%;
	}
	
	#search-block-form {
		padding-top:5px;
		padding-right:30px!important;
	}

	
	.main-nav.active-fixed {
		position:fixed!important;
		margin-left:0!important;
		float:left;
	}
	
	
	.main-nav {
		width:80%;
		position:relative!important;
	}
	
	.block.block-nodeblock.block-nodeblock-10.header {
		width:100%;
	}
	
	.block.block-nodeblock.contextual-links-region.block-nodeblock-10.header {
		width:100%;
	}
}



/*Save FIles*/
.portlet-content{
	float:left;
	width:100%;
}
.save-file{
	cursor: pointer;
	color:#36a7c9 !important;
}
.save-file:hover{
	cursor: pointer;
	color:#04c5ff !important;
}
.dropped{
	background: #d0d0d0;
}
.save{
	padding-top:5px;
	text-align: center;
}
.sort-saved-files{
	float:right;
}
.ui-save-active{
	
}

.save-files{
	box-shadow:0 5px 5px 5px #888;
	float:right;
	height:100px;
	display:none;
	position:fixed;
	top:200px;
	right:100px;
	width:130px !important;
	background: url(/sites/all/themes/fortified_dev/images/folder.png) no-repeat center transparent;
}






/* File Browser */
.file-browser-wrapper{
  padding-top:50px;
  .item-list{
    h3{
    }
    .view-file{
      cursor:pointer;
    }
    .view-file:hover{
        background:#f3f3f3;
      }
    .file-list{
      float:left;
      list-style-type: none;
    }
  }
}


#file-preview{
  padding-top:20px;
  list-style-type: none;
  li{
    cursor:pointer;
    max-width:200px;
    float:left;
  }
}

#file-preview{
  padding-top:20px;
}

.file-actions-wrapper{
  list-style-type: none;
  .file-actions{
    float:left;
    display:none;
  }
}
/* IP DATATEL ALARM */

.user-alarms-fieldset{
  margin-top:10px;
  margin-bottom:10px;
  .alarm-menu-wrapper{
  	list-style-type: none;
	  .alarm_login{
	  	float:left;
	  }
		li{
			a{
				color:#333333;
		    }
		  }
    }
  .user-alarm-wrapper{
    .alarm-user-list{
      margin-bottom:0.50em !important;
    }
  }
}




.event-logs-title{
  font-size:23px;
  padding-top:20px;
  padding-bottom:20px;
  font-family:$font;
  text-align:center;
}

.sort-events-submit-wrapper{
  padding-bottom:20px;
}
.sort-events-search-title{
  padding:10px;
}



